{
  "name": "vumi-go",
  "version": "0.5.0-dev",
  "homepage": "https://github.com/praekelt/vumi-go",
  "authors": [
    "Praekelt Foundation"
  ],
  "license": "BSD",
  "private": true,
  "dependencies": {
    "backbone": "~1.1.2",
    "jquery": "1.9.1",
    "jquery.cookie": "~1.3.1",
    "jquery-ui": "~1.10.3",
    "node-uuid": "~1.4.1",
    "js-base64": "~2.1.2",
    "jsPlumb": "git@github.com:praekelt/jsPlumb.git#~1.5.2",
    "lodash": "~2.0.0",
    "backbone-relational": "0.8.6",
    "bootstrap": "~3.0.0",
    "bootbox": "~4.0.0",
    "diamondash": "~0.3.1"
  },
  "devDependencies": {
    "chai": "~1.7.2",
    "sinon": "http://sinonjs.org/releases/sinon-1.7.3.js",
    "jquery-simulate": "git@github.com:jquery/jquery-simulate.git"
  },
  "exportsOverride": {
    "bootstrap": {
      "js": "dist/js/*",
      "css": "dist/css/*",
      "fonts": "dist/fonts/*"
    },
    "jquery-simulate": {
        "js": "jquery.simulate.js"
    },
    "jquery-ui": {
      "js": "ui/jquery-ui.js"
    },
    "diamondash": {
      "js": "diamondash/public/js/diamondash.js",
      "css": "diamondash/public/css/diamondash.css"
    },
    "backbone.babysitter": {
        "js": "lib/*.js"
    },
    "*": {
      "js": [
        "*.js",
        "js/*.js",
        "dist/**/*.js"
      ],
      "css": [
        "*.css",
        "css/*.css",
        "dist/**/*.css"
      ],
      "img": [
        "*.png",
        "*.jpg",
        "img/*.png",
        "img/*.jpg",
        "dist/**/*.png",
        "dist/**/*.jpg"
      ]
    }
  },
  "resolutions": {
    "jquery": "1.9.1"
  }
}
